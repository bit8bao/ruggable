{
  "name": "interview-api",
  "version": "1.0.0",
  "main": "index.ts",
  "_moduleAliases": {
    "@entity": "source/entity",
    "@shared": "source/shared",
    "@decorators": "source/decorators",
    "@types": "types.d.ts"
  },
  "scripts": {
    "generate": "graphql-codegen --config codegen.yml",
    "generate:watch": "nodemon --exec yarn generate -e source/graphql",
    "dev": "concurrently \"yarn generate:watch\" \"yarn start:watch\"",
    "orm": "ts-node -r module-alias/register ./node_modules/typeorm/cli.js",
    "start": "ts-node index.ts",
    "start:watch": "nodemon --exec yarn start -e ts",
    "sync:run": "yarn orm schema:sync",
    "sync:log": "yarn orm schema:log",
    "test": "jest --coverage --verbose --detectOpenHandles",
    "lint": "tslint -c ./tslint.json --project ./tsconfig.json --fix"
  },
  "dependencies": {
    "@apollo/federation": "0.9.4",
    "@nestjs/graphql": "^7.5.1",
    "apollo-server": "2.25.4",
    "apollo-server-core": "2.9.6",
    "aws-sdk": "^2.548.0",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dataloader": "^1.4.0",
    "express": "^4.17.1",
    "graphql-scalars": "^1.2.1",
    "graphql-tools": "^6.0.12",
    "http": "^0.0.0",
    "linq": "^3.2.1",
    "merge-graphql-schemas": "1.7.0",
    "module-alias": "^2.2.1",
    "pg": "^7.12.1",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.18",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.8.1",
    "@graphql-codegen/typescript": "1.8.1",
    "@graphql-codegen/typescript-resolvers": "1.8.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "concurrently": "^4.1.2",
    "git-branch-is": "^3.0.0",
    "husky": "^2.4.0",
    "jest": "^24.9.0",
    "jest-junit": "^8.0.0",
    "nodemon": "^1.19.2",
    "pretty-quick": "^1.11.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.10.2",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.5",
    "validator": "13.0.0"
  }
}
